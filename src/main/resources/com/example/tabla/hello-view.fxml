<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="299.0" prefWidth="268.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tabla.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TableView fx:id="tablaPersonas" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="namecolumn" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="ageColumn" prefWidth="75.0" text="Edad" />
            <TableColumn fx:id="emailColumn" prefWidth="75.0" text="Correo Electrónico" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#onaddpersonclick" text="añadirPersona" />
   </children>
</VBox>
